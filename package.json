{
  "name": "bitgraph",
  "version": "0.0.1",
  "description": "Starter project for an ES6 RESTful Express API",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "author": "Kenta Iwasaki",
  "license": "MIT",
  "dependencies": {
    "async": "^2.1.2",
    "asyncawait": "^1.0.6",
    "bluebird": "^3.4.6",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "csv": "^1.1.0",
    "csv-parser": "^1.11.0",
    "csv-write-stream": "^2.0.0",
    "density-clustering": "^1.3.0",
    "express": "^4.13.3",
    "finance-scraper-js": "0.0.1",
    "google-finance": "^0.1.7",
    "lodash": "^4.16.6",
    "mongodb": "^2.2.11",
    "resource-router-middleware": "^0.5.1",
    "tean-js": "^1.1.1",
    "traderjs": "^2.1.3",
    "yahoo-finance": "latest"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.1.1",
    "nodemon": "^1.9.2"
  },
  "keywords": [
    "stocks"
  ]
}
